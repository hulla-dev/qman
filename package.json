{
  "name": "qman",
  "author": "Samuel Hulla",
  "version": "0.0.5",
  "description": "Ultra lightweight query manager",
  "keywords": [
    "qman",
    "fetch",
    "fetch-manager",
    "request",
    "cache",
    "swr",
    "query",
    "hooks",
    "react",
    "vanilla",
    "react-query",
    "nanoquery"
  ],
  "packageManager": "pnpm@8.6.12",
  "license": "MIT",
  "repository": "github:samuelhulla/qman",
  "homepage": "https://hulla.dev/qman",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "bunchee": "^3.6.1",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.1",
    "typescript": "^5.1.6"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./core/dist/index.mjs",
      "module": "./core/dist/index.esm.js",
      "require": "./core/dist/index.js"
    }
  },
  "module": "./core/dist/index.esm.js",
  "types": "./core/dist/index.d.ts",
  "main": "./core/dist/index.js",
  "files": [
    "core/dist/**/*.{js,d.ts,mjs,d.mts}",
    "core/package.json"
  ],
  "scripts": {
    "build": "bunchee --dts -m --cwd core",
    "dev": "bunchee -w --cwd core"
  },
  "volta": {
    "node": "18.17.1"
  }
}
